{
  "name": "nestjs-microservice-api",
  "version": "0.0.1",
  "description": "NestJS microservice with Swagger API documentation",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:qa": "cross-env NODE_ENV=qa nest start --watch",
    "start:uat": "cross-env NODE_ENV=uat nest start --watch",
    "start:prod": "cross-env NODE_ENV=prod node dist/main",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:debug:qa": "cross-env NODE_ENV=qa nest start --debug --watch",
    "start:debug:uat": "cross-env NODE_ENV=uat nest start --debug --watch",
    "build:dev": "cross-env NODE_ENV=development nest build",
    "build:qa": "cross-env NODE_ENV=qa nest build",
    "build:uat": "cross-env NODE_ENV=uat nest build",
    "build:prod": "cross-env NODE_ENV=prod nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "cross-env NODE_ENV=development jest",
    "test:watch": "cross-env NODE_ENV=development jest --watch",
    "test:cov": "cross-env NODE_ENV=development jest --coverage"
  },
  "dependencies": {
    "@google-cloud/pubsub": "5.2.0",
    "node-fetch": "2.7.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.0",
    "axios": "^1.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^5.1.3"
  }
}
